{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "xo",
        "plugin:react/recommended"
    ],
    "overrides": [
        {
            "extends": [
                "xo-typescript"
            ],
            "files": [
                "*.ts",
                "*.tsx"
            ]
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "@typescript-eslint/no-unsafe-assignment": "warning",
        "no-unsafe-assignment": "off"
    }
}
